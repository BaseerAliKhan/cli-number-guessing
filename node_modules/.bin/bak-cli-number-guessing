#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//bin/usr/env" ]; then
  exec "$basedir//bin/usr/env" node "$basedir/../bak-cli-number-guessing/index.js" "$@"
else 
  exec /bin/usr/env node "$basedir/../bak-cli-number-guessing/index.js" "$@"
fi
